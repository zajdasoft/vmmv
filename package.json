{
  "name": "vmmv",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "pnpm --filter @vmmv-docs/test-app dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "eslint-config-custom": "workspace:*",
    "jest": "^29.4.3",
    "prettier": "latest",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "turbo": "latest",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.27.1"
}